   "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

   <description>Servlet Calculadora Web</description>

   <!-- SERVLETS -->

   <servlet>
      <servlet-name>Resultado</servlet-name>
      <servlet-class>uniandes.cupi2.calculadoraweb.interfaz.ServletCalculadora</servlet-class>
   </servlet>
   
   <!-- MAPPING -->
   
   <servlet-mapping>
      <servlet-name>Resultado</servlet-name>
      <url-pattern>/resultado.htm</url-pattern>
   </servlet-mapping>

</web-app>
